    "./lucky": {
      "import": {
        "types": "./dist/lucky/index.d.ts",
        "default": "./dist/lucky/index.js"
      },
      "require": {
        "types": "./dist/lucky/index.d.cts",
        "default": "./dist/lucky/index.cjs"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "dependencies": {
    "dotenv": "^17.2.2",
    "lodash": "^4.17.21",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/lodash": "^4.17.20",
    "rollup": "^4.50.1"
  },
  "scripts": {
    "tsc": "npx tsc -p tsconfig.json",
    "dev": "rollup -c rollup.config.mjs -w"
