        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./default": {
      "import": {
        "types": "./dist/default/index.d.ts",
        "default": "./dist/default/index.js"
      },
      "require": {
