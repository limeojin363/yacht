  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "yarn@4.9.1",
  "scripts": {
    "dev": "yarn workspace @yacht/client dev & yarn workspace @yacht/server dev",
